@model IList<Person>
<div class="card bg-primary text-white">
    <h1 class="display-4">Person </h1>
    <table class="table table-dark">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Contact</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <th>@item.Id</th>
                    <th>@item.Name</th>
                    <th>@item.Address</th>
                </tr>
            }
        </tbody>
    </table>
</div>
